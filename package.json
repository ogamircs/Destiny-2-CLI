{
  "name": "destiny-cli",
  "version": "0.1.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "destiny": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun run scripts/build.ts",
    "dev": "bun run --watch src/index.ts",
    "test": "find tests -name '*.test.ts' | sort | while read -r f; do bun test \"$f\" || exit 1; done"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "bungie-api-ts": "^5.10.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.3",
    "open": "^11.0.0",
    "ora": "^9.3.0"
  }
}
